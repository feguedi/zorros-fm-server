{
  "root": true,
  "env": {
    "node": true,
    "es2022": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "func-names": "off",
    "import/extensions": "off",
    "max-len": "off",
    "no-console": "off",
    "no-debugger": "off",
    "no-nested-ternary": "off",
    "no-param-reassign": "warn",
    "no-underscore-dangle": "off",
    "no-unresolved": "off"
  }
}
